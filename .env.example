# .env.example（复制到.env本地使用，.env已在.gitignore中）
# 基础配置
DEBUG=False
LOG_LEVEL=INFO
PORT=8000
DATA_DIR=data
MODELS_DIR=models
TEST_IMAGES_DIR=tests
MODEL_PATH=cnn_model.h5
BATCH_SIZE=128
EPOCHS=10
LEARNING_RATE=0.001

# API密钥（可选，若用不到可留空）
OPENAI_API_KEY=your-openai-key
CUSTOM_API_SECRET=your-custom-secret

# DAGsHub配置（关键）
MLFLOW_TRACKING_URI=https://dagshub.com/hanasakis/number_recognize.mlflow
DAGSHUB_USERNAME=hanasakis
DAGSHUB_TOKEN=你的DAGsHub Token（本地用，不提交git）